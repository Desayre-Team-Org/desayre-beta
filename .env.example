HIGGSFIELD_VIDEO_APP=""
HIGGSFIELD_REFERENCE_IMAGE_PARAM="reference_image_urls"

# App
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# Telemetry
TELEMETRY_ENABLED="0"
TELEMETRY_LOG_PATH="./telemetry.jsonl"
